<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <ul>
        <li>
        <% if (user) { %>
            <img src="<%= user.avatar %>" alt="nothing">
            <a href="/logout">Log Out</a>
        <% } else { %>
            <a href="/auth/google">Login with Google</a>
        <% } %>
        </li>
    </ul>
        <% books.forEach((b) => { %>
            <div>
                <img src="<%= b.coverArt %>" alt="Cover Art">
                <h2><%= b.title %></h2>
                <h5><%= b.genre.join(', ') %></h4>
                <h3><%= b.rating %></h3>
                <p><%= b.synopsis %></p>
            </div>
        <% }); %> 
</body>
</html>